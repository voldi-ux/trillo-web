{
  "name": "trilo",
  "version": "1.0.0",
  "description": "Landing page for trilo",
  "main": "index.js",
  "scripts": {
    "watch_css": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch_css",
    "compile_css": "node-sass sass/main.scss css/style.comp.css",
    "prefix_css": "postcss --use autoprefixer -b'last 5' css/style.comp.css -o css/style.prefix.css ",
    "compress_css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build": "npm-run-all compile_css  prefix_css compress_css"
  },
  "author": "Jonas",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.8.5",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1"
  }
}
